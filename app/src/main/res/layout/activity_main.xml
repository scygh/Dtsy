<?xml version="1.0" encoding="utf-8"?>
<com.moredian.entrance.guard.view.designview.ReboundScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/iv_come_back"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scaleType="centerCrop"
                    android:src="@mipmap/icon_top" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/margin_10"
                    android:text="@string/xfms"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/textSize_26"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAccent" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_160"
                    android:layout_margin="@dimen/margin_10">

                    <LinearLayout
                        android:id="@+id/main_ll1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/main_manualconsumption" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="@string/sdkk"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_ll2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/main_auto" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="@string/zdkk"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_ll3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/main_time" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="@string/sdde"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/margin_10"
                    android:text="@string/ryxx"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/textSize_26"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAccent" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_160"
                    android:layout_margin="@dimen/margin_10">

                    <LinearLayout
                        android:id="@+id/main_ll4"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/main_persons" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="@string/rygl"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_ll5"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/vouchercenter" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="@string/vouchercenter"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/margin_10"
                    android:text="@string/bbzx"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/textSize_26"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAccent" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_160"
                    android:layout_margin="@dimen/margin_10">

                    <LinearLayout
                        android:id="@+id/main_ll6"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/xiaofeibaobiao" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="消费报表"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_ll7"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/chongzhijiluicon" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="充值报表"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_ll11"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/tubiaozhuzhuangtu" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="一周报表"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/margin_10"
                    android:text="@string/xtsz"
                    android:textColor="@color/colorAccent"
                    android:textSize="@dimen/textSize_26"
                    android:textStyle="bold" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorAccent" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/height_160"
                    android:layout_margin="@dimen/margin_10">

                    <LinearLayout
                        android:id="@+id/main_ll8"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/main_machine_setting" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="@string/jqsz"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_ll9"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/main_net_setting" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="@string/wlsz"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/main_ll10"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="?android:attr/selectableItemBackground"
                        android:gravity="center"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/width_70"
                            android:layout_height="@dimen/height_70"
                            android:src="@mipmap/ps" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_10"
                            android:text="@string/personset"
                            android:textSize="@dimen/textSize_20" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>

    </FrameLayout>

</com.moredian.entrance.guard.view.designview.ReboundScrollView>